<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>java-servlet-example</application>
  <version>1</version>
  <threadsafe>true</threadsafe>
  <service>cloudsql</service>
  <env-variables>
    <env-var name="RDS_JDBC_URL" value="jdbc:mysql://google/skier-db" />
    <env-var name="RDS_USERNAME" value="cs6650-user" />
    <env-var name="RDS_PW" value="cs6650-user" />
    <env-var name="GOOGLE_CLOUD_SQL_INSTANCE" value="java-servlet-example:us-west2:skier-db" />
  </env-variables>
  <system-properties>
    <property name="cloudsql" value="jdbc:mysql://google/${database}?useSSL=false&amp;cloudSqlInstance=${INSTANCE_CONNECTION_NAME}&amp;socketFactory=com.google.cloud.sql.mysql.SocketFactory&amp;user=${user}&amp;password=${password}" />
  </system-properties>
  <use-google-connector-j>true</use-google-connector-j>
  <runtime>java8</runtime>
</appengine-web-app>